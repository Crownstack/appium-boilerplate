# Environment Configuration
# Copy this file to .env and update with your actual values

# Test Environment (dev, qa, stg)
TEST_ENV=qa

# Platform Configuration (android, ios)
PLATFORM=android

# Login Credentials
USERNAME=your_valid_username
PASSWORD=your_valid_password

# Android Configuration
ANDROID_VERSION=13
ANDROID_DEVICE=emulator-5554
ANDROID_APP_PATH=/path/to/your/android/app.apk
APP_PACKAGE=com.example.app
APP_ACTIVITY=com.example.MainActivity

# iOS Configuration
IOS_VERSION=16.0
IOS_DEVICE=iPhone 14
IOS_APP_PATH=/path/to/your/ios/app.app
APP_BUNDLE_ID=com.example.app

# Appium Server
APPIUM_SERVER_URL=http://localhost:4723/wd/hub
